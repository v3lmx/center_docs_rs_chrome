{
    "name": "Center docs.rs",
    "description": "Center docs.rs content in the browser page",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://docs.rs/*"
            ],
            "css": [
                "center.css"
            ]
        }
    ],
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}
